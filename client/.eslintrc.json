{
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-console": 0,
    "no-debugger": 0,
    "no-await-in-loop": 0,
    "no-unused-vars": [
      1,
      {
        "argsIgnorePattern": "res|next|^err"
      }
    ],
    "no-shadow": [
      2,
      {
        "hoist": "all",
        "allow": [
          "resolve",
          "reject",
          "done",
          "next",
          "err",
          "error",
          "Component"
        ]
      }
    ],
    "no-restricted-syntax": [
      2,
      "ForInStatement",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "react/prop-types": 0,
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0,
    "react/state-in-constructor": 0,
    "jsx-a11y/no-autofocus": 0,
    "react/prefer-stateless-function": 0,
    "react/no-unescaped-entities": [
      2,
      {
        "forbid": [
          ">",
          "}"
        ]
      }
    ],
    "react/jsx-curly-newline": 0,
    "react/jsx-indent": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/jsx-one-expression-per-line": 0
  },
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:prettier/recommended"
  ]
}